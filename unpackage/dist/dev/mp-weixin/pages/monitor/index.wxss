.addheader {
  width: 100%;
  height: 186rpx;
  position: relative;
}
.addheaderbac {
  width: 100%;
  height: 122rpx;
  background: #266CEB;
}
.device-tip {
  width: calc(100% - 60rpx);
  height: 140rpx;
  background: #FFFFFF;
  border: 1rpx solid #FFFFFF;
  box-shadow: 0px 0px 35rpx 0px rgba(72, 108, 224, 0.13);
  border-radius: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: absolute;
  bottom: 0;
  left: 30rpx;
}
.devicenum {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  color: #999999;
  font-size: 24rpx;
}
.devicenum text {
  display: block;
  color: #266CEB;
  font-size: 40rpx;
  font-weight: 600;
  margin-bottom: 10rpx;
}
.device-tip .addline {
  width: 1rpx;
  height: 44rpx;
  background: #000000;
  opacity: 0.1;
}
.page-body {
  padding: 0 30rpx;
}
.search-store {
  margin: 41rpx 0;
  clear: both;
  overflow: hidden;
}
.search-store input {
  width: 602rpx;
  height: 68rpx;
  background: #F0F5F9;
  box-shadow: 0px 13rpx 13rpx 0rpx rgba(240, 245, 249, 0.23);
  border-radius: 10rpx;
  padding: 0 20rpx;
  font-size: 28rpx;
  color: #8FA4B7;
  box-sizing: border-box;
  float: left;
}
.search-store view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  float: left;
  background: #33CE55;
  margin-left: 20rpx;
}
.search-store view uni-icon {
  font-size: inherit;
}
.search-store .uni-collapse-cell__title-img {
  width: 28rpx;
  height: 34rpx;
}
.check_cyList {
  position: relative;
}
.check_cyList .selectcheck {
  float: left;
  font-size: 36rpx;
  color: #333333;
  font-weight: 500;
}
.check_cyList .selectcheck image {
  width: 28rpx;
  height: 34rpx;
  margin: 5rpx 10rpx 0 0;
}
.check_cyList .selectcheck .uni-icons {
  margin-left: 10rpx;
}
.check_cyList .rightcheck {
  background: #F0F5F9;
  float: right;
}
.check_cyList .rightcheck image {
  width: 38rpx;
  height: 38rpx;
}
.check_cyList .uni-collapse-cell__title {
  padding: 12px 0;
}
.check_cyList .uni-collapse-cell__title .uni-collapse-cell__title-img {
  width: 28rpx;
  height: 34rpx;
  margin-right: 10rpx;
}
.check_cyList .checklist {
  margin-top: 20rpx;
  position: absolute;
  width: 100%;
  z-index: 2;
  height: 336rpx;
  overflow-y: auto;
}
.shop_type {
  width: 100%;
  background: #F0F5F9;
  border-radius: 10rpx;
  margin-top: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.shop_type view:first-child {
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}
.shop_type view:last-child {
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.shop_type .reqa {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 78rpx;
  font-size: 32rpx;
  color: #8FA4B7;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.shop_type .addceline {
  width: 2rpx;
  height: 78rpx;
  background: #FFFFFF;
}
.shop_type .select_type.reqa {
  background: #266CEB;
  color: #FFFFFF;
}

